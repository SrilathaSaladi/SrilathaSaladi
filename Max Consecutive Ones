var findMaxConsecutiveOnes = function(nums) {
     var count =0;
    var temp = 0;
    for(var i=0; i<nums.length; i++){
       
      if(nums[i] == '1'){
          count++;
          if(temp<count){
          temp = count;
        }
    }
        else{
           count =0;
            
        }
    }
    return temp;
};
